{
  "name": "base",
  "description": "base is the foundation for creating modular, unit testable and highly pluggable node.js applications, starting with a handful of common methods, like `set`, `get`, `del` and `use`.",
  "version": "0.6.7",
  "homepage": "https://github.com/node-base/base",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Brian Woodward (https://github.com/doowb)"
  ],
  "repository": "node-base/base",
  "bugs": {
    "url": "https://github.com/node-base/base/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "class-utils": "^0.3.0",
    "collection-visit": "^0.2.1",
    "component-emitter": "^1.2.0",
    "define-property": "^0.2.5",
    "get-value": "^2.0.3",
    "lazy-cache": "^1.0.3",
    "set-value": "^0.3.3",
    "to-object-path": "^0.2.0",
    "unset-value": "^0.1.1"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.0.0",
    "gulp-format-md": "^0.1.5",
    "gulp-istanbul": "^0.10.0",
    "gulp-mocha": "^2.1.3",
    "mocha": "*",
    "should": "*"
  },
  "keywords": [
    "boilerplate",
    "cache",
    "del",
    "get",
    "inherit",
    "methods",
    "set",
    "starter",
    "unset",
    "visit"
  ],
  "verb": {
    "plugins": [
      "gulp-format-md"
    ],
    "layout": "default",
    "related": {
      "description": "There are a number of different plugins available for extending base. Let us know if you create your own!",
      "list": [
        "base-argv",
        "base-cli",
        "base-config",
        "base-cwd",
        "base-data",
        "base-fs",
        "base-list",
        "base-options",
        "base-pipeline",
        "base-plugins",
        "base-questions",
        "base-runner",
        "base-store",
        "base-tasks",
        "base-tree"
      ]
    }
  }
}
